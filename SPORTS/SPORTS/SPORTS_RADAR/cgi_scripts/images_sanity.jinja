    $(document).ready(function() { $('#IMAGESANITY').dataTable({
        "bJQueryUI"      : true,
        "iDisplayLength" : 20,
        "aoColumns"      : [ null, null, null, null, { "sType": 'html' } ],
       });
    });
</script>-->

<script type="text/javascript" charset="utf-8">
$(document).ready(function()
    {
        $('#myTable').tablesorter();
    }
);
</script>

</body>
</html>
