<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN">
<html lang="en">
<meta charset="utf-8"/>
<head>
<title>ALARM INFORMATION</title>
</head>

<!-- Bootstrap -->
<link href="/css/bootstrap.css" rel="stylesheet" media="screen">
<link href="/css/demo_table.css" rel="stylesheet" media="screen">
<link href="/css/jquery_custom.css" rel="stylesheet" media="screen">

</head>

<body>
<div>
<a name="index"/>
<h2 name="index" align="center">DATA FLOW at {{today_date}}<h2>
</div>

{% for entity, alarm_list in ssc_vkc_info.items() %}

<div class ="span12">
<table ID="ALARMS" class="table table-bordered table-hover table-striped">

<thead>
<tr>
   <th rowspan = 2 align = 'center'>INDEX</th>
   <th rowspan = 2 align = 'center'>DATA ID</th>
   <th colspan = 2 align = 'center'>IP</th>
   <th rowspan = 2 align = 'center'>TGZ</th>
   <th rowspan = 2 align = 'center'>DAYS</th>
   <th rowspan = 2 align = 'center'>STATUS</th>
   <th colspan = 3 align = 'center'>PUSH DATA</th>
   <th colspan = 3 align = 'center'>RUN DATA</th>
   <th colspan = 3 align = 'center'>UPGRADE DATA</th>
</tr>
<tr>
   <th>VKC</th>
   <th>SSC</th>
   <th>START</th>
   <th>END</th>
   <th>STATUS</th>
   <th>START</th>
   <th>END</th>
   <th>STATUS</th>
   <th>START</th>
   <th>END</th>
   <th>STATUS</th>
</tr>
</thead>

<tbody>
{% for row in alarm_list %}
    <tr>
        <td align=center>{{loop.index}}</td>
    {% for value in row %}
        <td>{{value}}</td>
    {% endfor  %}
    </tr>
{% endfor  %}
</tbody>

</table>
</div>


{% endfor  %}

<script src="/js/jquery.js" type="text/javascript"></script>
<script src="/js/jquery.dataTables.js" type="text/javascript"></script>
<script src="/js/bootstrap.min.js" type="text/javascript"></script>

<script type="text/javascript" charset="utf-8">
    $(document).ready(function() { $('#ALARMS').dataTable({ "bJQueryUI" : true, "iDisplayLength" : 20, }); });
</script>


</body>
</html>
